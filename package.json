	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build --verbose",
		"build:local": "env-cmd -f .env.local react-scripts build",
		"build:development": "env-cmd -f .env.development react-scripts build",
		"build:staging": "env-cmd -f .env.staging react-scripts build",
		"build:production": "env-cmd -f .env.production react-scripts build",
		"test": "cross-env CI=true react-scripts test --color",
		"eject": "react-scripts eject",
		"test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
		"start:prod": "yarn run build && serve -s build",
		"checkTs": "tsc --noEmit",
		"eslint": "eslint --ext js,ts,tsx src",
		"lint": "yarn run eslint --ext js,ts,tsx src",
		"lint:fix": "yarn run eslint --fix --ext js,ts,tsx src",
		"lint:css": "stylelint src/**/*.css",
		"generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
		"cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
		"prettify": "prettier --write",
		"extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
		"prepare": "husky install"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"engines": {
		"yarn": ">=1.22.10",
		"node": ">=14.0.0"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"yarn run eslint --ext ts,tsx --fix  src/"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*/*.d.ts",
			"!src/**/*/Loadable.{js,jsx,ts,tsx}",
			"!src/**/*/messages.ts",
			"!src/**/*/types.ts",
			"!src/index.tsx"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90,
				"functions": 90,
				"lines": 90,
				"statements": 90
			}
		}
	}
}
